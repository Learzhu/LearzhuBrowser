<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/headera_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:ellipsize="end"
        android:text="跳转到动画Acivity"
        android:visibility="visible" />

    <TextView
        android:id="@+id/headerb_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="将Excel 写入到SQLite"
        android:visibility="visible" />

    <View
        android:id="@+id/headerc"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="CCCCCCCCCCCCCCCCCCCCCC"
        android:visibility="visible" />

    <TextView
        android:id="@+id/headerd_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="跳转到RecyclerView的界面"
        android:visibility="visible" />

    <TextView
        android:id="@+id/headere_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="跳转到AsyncTask的界面"
        android:visibility="visible" />

    <TextView
        android:id="@+id/headerf_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试Dialog"
        android:visibility="visible" />

    <TextView
        android:id="@+id/headerg_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试垂直TabLayout"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textswitcher"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试TextSwitcher"
        android:visibility="visible" />

    <TextView
        android:id="@+id/text_open_o_service"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试O以及以上系统的service"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tts_tv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试Android_TTS"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tv_stt"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试Android_live_transcribe"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tv_notification"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试Android Notification"
        android:visibility="visible" />

    <TextView
        android:id="@+id/tv_share"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="测试ShareSDK 分享"
        android:visibility="visible" />
</LinearLayout>
